{
    "files.associations": {
        "iostream": "cpp",
        "stdio.h": "c"
    }
    , "code-runner.executorMap" : {
        "cpp": "cd $dir && g++ $fileName -o $workspaceRoot\\bin\\$fileNameWithoutExt.exe && $workspaceRoot\\bin\\$fileNameWithoutExt.exe"
        ,"c": "cd $dir && gcc $fileName -o $workspaceRoot\\bin\\$fileNameWithoutExt.exe && $workspaceRoot\\bin\\$fileNameWithoutExt.exe"
    }
}